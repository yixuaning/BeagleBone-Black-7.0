# @name USB host device w/r stress test with fs:ext2 and size:10M (bs=10M;count=1)
# @desc Perform mkfs first, then do read write 
#	The device node is specified in get_blk_device_node.sh
# @requires usb/host || usbxhci/host
# @setup_requires usbhostmsc

USBHOST_L_STRESS_EXT2_DD_RW_0001 source 'common.sh'; do_cmd install_modules.sh 'usb'; DEV_NODE=`get_blk_device_node.sh "usb"` || die "error getting devnode for usb"; do_cmd blk_device_dd_readwrite_test.sh -f "ext2" -b "10M" -c "1" -d "usb" -l "100" 
USBXHCIHOST_L_STRESS_EXT2_DD_RW_0001 source 'common.sh'; do_cmd install_modules.sh 'usbxhci'; DEV_NODE=`get_blk_device_node.sh "usbxhci"` || die "error getting devnode for usbxhci"; do_cmd blk_device_dd_readwrite_test.sh -f "ext2" -b "10M" -c "1" -d "usbxhci" -l "100" 





