# @name SPI raw write test
# @desc Write to SPI flash directly
# @requires spi_master/mtd

SPI_L_FUNC_MTD_RAW_WRITE_CHAR source "common.sh"; install_modules.sh "spi"; part=`get_mtd_partition_number.sh "spi"` || die "error getting mtd part number: $part"; do_cmd "echo deadbeaf > /home/root/short_test "; do_cmd "dd if=/home/root/short_test of=/dev/mtd"$part" " 
SPI_L_FUNC_MTD_RAW_WRITE_BLOCK source "common.sh"; install_modules.sh "spi"; part=`get_mtd_partition_number.sh "spi"` || die "error getting mtd part number: $part"; do_cmd "echo deadbeaf > /home/root/short_test "; do_cmd "dd if=/home/root/short_test of=/dev/mtdblock"$part" " 



