NAND_L_FUNC_MTD_RAW_WRITE_CHAR source "common.sh"; install_modules.sh "nand"; part=`get_mtd_partition_number.sh "nand"` || die "error getting mtd part number: $part"; do_cmd "time dd if=/dev/urandom of=/dev/mtd"$part" bs=1M count=10" 
NAND_L_FUNC_MTD_RAW_WRITE_BLOCK source "common.sh"; install_modules.sh "nand"; part=`get_mtd_partition_number.sh "nand"` || die "error getting mtd part number: $part"; do_cmd "echo deadbeaf > /home/root/short_test "; do_cmd "dd if=/home/root/short_test of=/dev/mtdblock"$part" " 
#NAND_L_NANDWRITE_CMD source "common.sh"; install_modules.sh "nand"; x=0; loop=100; while [ $x -le $loop ]; do nandwrite -p /dev/mtd1 /media/mmcblk0p1/MLO; x=$((x+1)); sleep 1; done



