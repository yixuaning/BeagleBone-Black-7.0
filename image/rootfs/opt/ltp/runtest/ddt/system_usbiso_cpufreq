# @name Test usb driver while changing cpu frequency
# @desc Test usb driver while the OPP is changed to cover all possible vallues
# @requires usb || usbxhci
# @setup_requires usbhostaudio_usbhostvideo

SYSTEM_M_FUNC_OPP_USB_ISO source 'common.sh'; run_processes.sh -c "/opt/ltp/runltp -f ddt/usbhost_iso -P $MACHINE -s USBHOST_S_FUNC_AUDIO_LOOPBACK_WITH_VIDEO_CAPTURE # /opt/ltp/runltp -P $MACHINE -f ddt/powermgr_cpufreq -s POWERMGR_S_FUNC_CPUFREQ_BASIC"
SYSTEM_M_FUNC_OPP_USBXHCI_ISO source 'common.sh'; run_processes.sh -c "/opt/ltp/runltp -f ddt/usbhost_iso -P $MACHINE -s USBXHCIHOST_S_FUNC_AUDIO_LOOPBACK_WITH_VIDEO_CAPTURE # /opt/ltp/runltp -P $MACHINE -f ddt/powermgr_cpufreq -s POWERMGR_S_FUNC_CPUFREQ_BASIC"

