# @name 
#	NAND insmod rmmod test
# @desc 
#       modprobe nand module and then rmmod it
# @requires  nand 

NAND_M_MODULAR_INSMOD_RMMOD source "common.sh"; install_modules.sh "nand"; mod_name=`get_modular_name.sh "nand"` || die "error getting modular name: $mod_name"; do_cmd insmod.sh "$mod_name"; do_cmd rmmod.sh "$mod_name" 
