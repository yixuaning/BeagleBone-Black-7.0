# @name RTC readtime modular stress test
# @desc Insert module, then do readtime test, then remove module multiple times
# @requires rtc 

RTC_L_MODULAR_STRESS_READTIME_0001 source 'common.sh'; DEV_NODE=`get_devnode.sh "rtc"` || die "error getting devnode for rtc"; do_cmd 'do_modular_common.sh -d "rtc" -a "do_cmd rtc_tests -device $DEV_NODE  -ioctltest readtime"' -l "3"
