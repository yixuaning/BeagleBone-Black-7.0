# @name suspend/standby resume tests
# @desc Test suspend/standby resume after removing usb module
# @requires power_mgr && usb

POWERMGR_S_FUNC_SIMPLE_SUSPEND_REMOVE_USB source 'functions.sh'; usbdev=`get_modular_name.sh usb`; suspend -i 2 -p "mem" -u 1 -m $usbdev

POWERMGR_M_FUNC_SIMPLE_SUSPEND_REMOVE_USB source 'functions.sh'; usbdev=`get_modular_name.sh usb`; suspend -i 10 -p "mem" -u 1 -m $usbdev

POWERMGR_L_STRESS_SIMPLE_SUSPEND_REMOVE_USB source 'functions.sh'; usbdev=`get_modular_name.sh usb`; suspend -i 100 -p "mem" -u 1 -m $usbdev

POWERMGR_S_FUNC_SIMPLE_STANDBY_REMOVE_USB source 'functions.sh'; usbdev=`get_modular_name.sh usb`; suspend -i 2 -p "standby" -u 1 -m $usbdev

POWERMGR_M_FUNC_SIMPLE_STANDBY_REMOVE_USB source 'functions.sh'; usbdev=`get_modular_name.sh usb`; suspend -i 10 -p "standby" -u 1 -m $usbdev

POWERMGR_L_STRESS_SIMPLE_STANDBY_REMOVE_USB source 'functions.sh'; usbdev=`get_modular_name.sh usb`; suspend -i 100 -p "standby" -u 1 -m $usbdev
