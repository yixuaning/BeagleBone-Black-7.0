# @name Test mmc driver while changing cpu frequency
# @desc Test mmc driver while the OPP is changed to cover all possible vallues
# @requires mmc_host 

SYSTEM_S_FUNC_OPP_MMC source 'common.sh'; run_processes.sh -c " /opt/ltp/runltp -f ddt/mmc_dd_rw -P $MACHINE -s 'MMC_S_FUNC_DD_RW_QUICK '  # /opt/ltp/runltp -P $MACHINE -f ddt/powermgr_cpufreq -s POWERMGR_S_FUNC_CPUFREQ_BASIC "

SYSTEM_M_FUNC_OPP_MMC source 'common.sh'; run_processes.sh -c " /opt/ltp/runltp -f ddt/mmc_dd_rw -P $MACHINE -s 'MMC_M_FUNC_DD_RW_20TIMES ' # /opt/ltp/runltp -P $MACHINE -f ddt/powermgr_cpufreq -s POWERMGR_M_FUNC_CPUFREQ_BASIC "

