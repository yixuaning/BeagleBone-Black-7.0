# @name Graphics demos
# @desc Running various graphics demos
# @requires graphics

SGX_S_FUNC_BLIT source 'common.sh'; which sgx_blit_test || die "sgx_blit_test is not installed"; do_cmd 'sgx_blit_test -t' 
