# @name Nand jffs2 write stress test using dd with dd_cnt being half of mtd size 
# @desc Perform nand eraseall first, then do read write 
#	The partition number is specified in get_mtd_partition_number.sh
# @requires nand && jffs2
NAND_L_STRESS_DD_RW_JFFS2 source "common.sh"; install_modules.sh "nand"; part=`get_mtd_partition_number.sh "nand"` || die "error getting mtd part number: $part"; do_cmd blk_device_dd_readwrite_test.sh -f "jffs2" -b "1M" -c "100" -l "400" -d "nand"





