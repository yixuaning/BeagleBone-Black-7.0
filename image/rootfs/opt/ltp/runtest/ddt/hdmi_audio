# @name HDMI Audio . 
# @desc Test cases corresponding to HDMI playback Audio cases. 
# @requires hdmi/audio/playback

HDMI_M_FUNC_AUDIO_PLAYBACK_1080p60_32KHz  source 'common.sh' ; devnode=`get_hdmi_audio_devnode.sh -m "1080p-60"`; do_cmd 'alsa_tests.sh -D "hw:'$devnode'" -t playback -a 1 -d 10 -F /home/root/HDMI_M_FUNC_AUDIO_PLAYBACK_1080p60_32KHz.wav -r 32000'
HDMI_M_FUNC_AUDIO_PLAYBACK_1080i60_32KHz  source 'common.sh' ; devnode=`get_hdmi_audio_devnode.sh -m "1080i-60"`; do_cmd 'alsa_tests.sh -D "hw:'$devnode'" -t playback -a 1 -d 10 -F /home/root/HDMI_M_FUNC_AUDIO_PLAYBACK_1080i60_32KHz.wav -r 32000'
