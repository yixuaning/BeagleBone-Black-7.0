# @name 
#	SPI insmod rmmod test
# @desc 
#       modprobe spi module and then rmmod it
# @requires  spi_master 

SPI_M_MODULAR_INSMOD_RMMOD source "common.sh"; install_modules.sh "spi"; mod_name=`get_modular_name.sh "spi"` || die "error getting modular name: $mod_name"; do_cmd insmod.sh "$mod_name"; do_cmd rmmod.sh "$mod_name" 
